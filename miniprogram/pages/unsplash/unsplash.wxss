.unsplash_warp {
  display: flex;
  flex-direction: column;
  margin: 10rpx 0rpx 30rpx 0rpx;
}

.PiconLogo{
font-size: 80rpx;
font-weight: bold;
margin-left: 30rpx;
}

.unsplash_logo_warp image {
  width: 190.6px;
  height: 43.5px;
  margin: 24rpx 24rpx 0 30rpx;
}

.unsplash_logo_warp text {
  font-size: 30rpx;
  margin: 0 24rpx 10rpx 30rpx;
  color: #111111;
  font-weight: bold;
}

.card_warp {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin: 25rpx 25rpx 48rpx 25rpx;
}

.card_topic_warp,
.card_collection_warp {
  height: 200rpx;
  width: 48%;
  background-color: #fff;
  border-radius: 24rpx;
  border: 2rpx solid #e8e8e8;
  box-shadow: 5rpx 5rpx 5rpx 5rpx #f1f1f1;
}

.card_topic_title,
.card_collection_title,
.card_topic_body,
.card_collection_body {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin: 24rpx 16rpx 24rpx 24rpx;
}

.card_topic_title_text,
.card_collection_title_text {
  font-size: 32rpx;
  color: #202020;
  font-weight: bold;
}

.card_topic_title_icon,
.card_collection_title_icon {
  width: 28rpx;
  height: 28rpx;
  margin: 6rpx 12rpx 0rpx 0rpx;
}

.card_topic_body_text,
.card_collection_body_text {
  width: 170rpx;
  font-size: 26rpx;
  color: #202020;
}

.card_topic_body_icon,
.card_collection_body_icon {
  margin-top: 10rpx;
  width: 80rpx;
  height: 80rpx;
}

.unsplash_list_text {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin: 0rpx 30rpx 20rpx 30rpx;
}

.unsplash_list_layout_1 {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.unsplash_list_layout_1_icon {
  width: 60rpx;
  height: 60rpx;

}

.unsplash_list_layout_1_text{
  font-size: 36rpx;
  color: #111111;
  margin-left: 10rpx;
  font-weight: bold;
}

/* .unsplash_list_text_text {
  font-size: 32rpx;
  color: #111111;
  font-weight: bold;
} */

.unsplash_list {
  display: flex;
  padding: 10rpx;
  justify-content: space-around;
}

.unsplash_item {
  width: 340rpx;
  margin-top: 20rpx;
  border-radius: 24rpx;
  /* background-color: #fff; */
}

.unsplash_img image {
  width: 100%;
  border-radius: 24rpx 24rpx 24rpx 24rpx;
}
